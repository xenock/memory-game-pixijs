{
  "name": "memory_game",
  "version": "1.0.0",
  "description": "The wonderful memory game is here again!",
  "main": "build/index.html",
  "author": "Miguel Romero Moracho",
  "license": "MIT",
  "repository": "https://github.com/xenock/memory-game-pixijs",
  "keywords": [
    "pixi",
    "pixi.js",
    "pixi js",
    "pixi 2d",
    "pixi engine",
    "2d engine",
    "canvas engine",
    "game",
    "javascript game",
    "bootstrap",
    "es6",
    "babel",
    "pixi es6",
    "pixi babel",
    "pixi build",
    "pixiv4",
    "pixi v4",
    "pixi setup",
    "pixi example"
  ],
  "scripts": {
    "start": "NODE_ENV=dev && npm run watch",
    "watch": "webpack --config webpack.config.js --progress --inline --colors --watch",
    "build": "npm run build-production",
    "build-production": "SET NODE_ENV=production && npm run build-app",
    "build-dev": "SET NODE_ENV=dev && npm run build-app",
    "build-app": "webpack --config webpack.config.js --progress --colors -p",
    "clean": "rm -fr build/app.js build/app.js.map"
  },
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.13",
    "brfs": "^1.4.1",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "eslint": "^4.7.2",
    "eslint-loader": "^1.9.0",
    "json-loader": "^0.5.4",
    "pixi.js": "^4.8.2",
    "transform-loader": "^0.2.3",
    "webpack": "^1.12.9",
    "webpack-notifier": "^1.5.0"
  }
